<% @registrations.each do |r| %>
    <% full_team = "#{r.player.team.full_name if r.player.team}" %>
    <% guardian_emails = Guardian.find_all_by_player_id(r.player.id).map{|g| g.email}.join(',') %>
    <% team_slug= r.player.team.nil? ? team(r.school) : r.player.team.slug %>
    <% regdump = '' %>
    <% regdump += "#{r.section}\t#{r.last_name}, #{r.first_name}\t#{r.player.team.name if r.player.team}\t" %>
    <% regdump += "#{r.school}\t#{r.grade}\t#{r.uscf_member_id}\t#{r.rating}\t#{r.gender}\t#{r.date_of_birth}\t" %>
    <% regdump += "#{r.address}\t#{r.city}\t#{r.state}\t#{r.zip_code}\t#{full_team}\t0\t#{guardian_emails}\t#{team_slug}\n" %>
    <%= regdump %>
<% end %>